{
	"name": "next-todo",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "apps/next-todo",
	"projectType": "application",
	"targets": {
		"build": {
			"executor": "@nrwl/next:build",
			"outputs": ["{options.outputPath}"],
			"defaultConfiguration": "production",
			"options": {
				"root": "apps/next-todo",
				"outputPath": "dist/apps/next-todo"
			},
			"configurations": {
				"development": {
					"outputPath": "apps/next-todo"
				},
				"production": {}
			}
		},
		"serve": {
			"executor": "@nrwl/next:server",
			"defaultConfiguration": "development",
			"options": {
				"buildTarget": "next-todo:build",
				"dev": true,
        "port": 4202
			},
			"configurations": {
				"development": {
					"buildTarget": "next-todo:build:development",
					"dev": true
				},
				"production": {
					"buildTarget": "next-todo:build:production",
					"dev": false
				}
			}
		},
		"export": {
			"executor": "@nrwl/next:export",
			"options": {
				"buildTarget": "next-todo:build:production"
			}
		},
		"test": {
			"executor": "@nrwl/jest:jest",
			"outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
			"options": {
				"jestConfig": "apps/next-todo/jest.config.ts",
				"passWithNoTests": true
			}
		},
		"lint": {
			"executor": "@nrwl/linter:eslint",
			"outputs": ["{options.outputFile}"],
			"options": {
				"lintFilePatterns": ["apps/next-todo/**/*.{ts,tsx,js,jsx}"]
			}
		}
	},
	"tags": []
}
